{
  "name": "discord-bot-templet",
  "description": "a discord bot template",
  "version": "1.0.2",
  "main": "out/registry.js",
  "repository": "https://github.com/ThatGuyJamal/discord-bot-templet.git",
  "author": "ThatGuyJamal",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "launch": "node out/registry.js",
    "format": "prettier --ignore-path .gitignore --write .",
    "dev": "concurrently \"yarn watch\" \"yarn launch\"",
    "make": "yarn build && yarn format"
  },
  "keywords": [
    "discord",
    "discord.js",
    "discord-bot-templet"
  ],
  "engines": {
    "node": ">=16.6",
    "npm": ">=7"
  },
  "devDependencies": {
    "@types/ms": "^0.7.31",
    "pm2": "^5.1.2",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@sapphire/decorators": "^4.2.3",
    "@sapphire/framework": "^3.0.0-next.73cfe9f.0",
    "@sapphire/plugin-hmr": "^1.1.1",
    "@sapphire/plugin-i18next": "^2.2.3",
    "@sapphire/plugin-logger": "^2.1.3",
    "@sapphire/plugin-subcommands": "^2.1.3",
    "@sapphire/time-utilities": "^1.6.0",
    "@typegoose/typegoose": "^9.6.2",
    "discord.js": "^13.6.0",
    "mongoose": "^7.0.0",
    "ms": "^2.1.3",
    "reflect-metadata": "^0.1.13"
  }
}
